.modal-header
  %h1
    Your Gmail contacts
.modal-body
  %span
  #list_of_contacts
    %table{:id => "fetched_gmail_contacts_list", :width => "100%"}
      -all_contacts.each do |contact|
        %tr
          %td
            %input{:type=>"checkbox", :name=>"contact_list", :id => contact}
          %td
            = contact

#pageNavPosition

.modal-footer
  %span{:class => 'btn btn-primary btn-large pull-left', :id => 'add-contacts'} Invite
  
%script{:type => "text/javascript"}
  var pager = new Pager('fetched_gmail_contacts_list', 10, 'pager', 'pageNavPosition');
  pager.init();
  pager.showPage(1);
